$('#calendar').fullCalendar({
	header: {
		left: 'prev,next today',
		center: 'title',
		right: 'month,basicWeek,basicDay'
	},
	editable: true,
	events: [
	<% @events.each do |event| %>
		{
			title: '<%= event.title %>',
			start: new Date(y, m, 1)
		},
	<% end %>
	]
});


 $('#calendar').fullCalendar({
    dayClick: function() {
      alert('a day has been clicked!');
  }
 })
